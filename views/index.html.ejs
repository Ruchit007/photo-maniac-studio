                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="active"><a id="home" href="/home"> HOME <span class="sr-only">(current)</span></a></li>
                        <li class="hidden-xs hidden-sm"><a id="gallery" href="/gallery">GALLERY</a></li>
                        <li><a id="about" href="/about">ABOUT</a></li>
                        <li><a id="contact" href="/contact">CONTACT</a></li>
                        <li class="hidden-xs hidden-sm"><a id="admin" href="/login">ADMIN</a></li>
                    </ul>
                    </div><!-- /.navbar-collapse -->
                </div><!-- /.container-fluid -->
            </nav>


        </div>

        <!--<div class="container-fluid">-->

            <div id="message">
