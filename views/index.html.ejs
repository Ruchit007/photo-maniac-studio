            </div>

            <div ng-view></div>
            
        <!--</div>-->

        <div class="window-footer">
            <div class="container">
                <div class="col-md-4 col-sm-6 mobile">
                    <img src="images/call.png" alt="" />
                    <p class="mobile-text">+91 851191 7777</p>
                    <p class="whatsapp-text">+91 851191 7777</p>
                    <img src="images/whatsapp.png" alt="" />        
                </div>
                <div class="col-md-5 address visible-md visible-lg">
                    <img src="images/address.png" alt="" />
                    <p class="address-text">PhotoManiac Studio, Rameshvar Complex, Opp. Jayavardhini Commercial, Sadhu vasvani Road, Rajkot - 360001</p>
                </div>
                <div class="col-md-3 col-sm-6 other visible-sm visible-md visible-lg">
                    <div class="row">
                        <a href="https://www.facebook.com/Photomaniac-Studio-1888731094738020/" class="faccebook">
                            <img ng-src="images/facebook.png" />                        
                        </a>
                        <a href="https://www.instagram.com/photomaniac_studio/">                    
                            <img ng-src="images/instagram.png" />
                        </a>
                    </div>
                    <div class="row mail">
                        <img src="images/mail.png" alt="">
                        <p class="mail-text">photomaniacstudio7@gmail.com</p>
                    </div>
                </div>
            </div>
